---
import HeaderLink from "./HeaderLink.astro";
import Icon from "./Icon.astro";
import { SITE_TITLE } from "../config";
import { getBaseUrl } from "../util/getBaseUrl";
const { default: innerHTML } = await import("/placeholder-logo.svg?raw");
---

<header>
	<nav>
		<HeaderLink href={getBaseUrl("/")}>
			<Icon src="/placeholder-logo.svg?raw" name="logo" title="logo" />
			<Fragment set:html={innerHTML} />
		</HeaderLink>
		<HeaderLink />
		<HeaderLink />
		<HeaderLink>Support</HeaderLink>
		<HeaderLink>Log in / Sign up</HeaderLink>
	</nav>
</header>
<style>
	header {
		margin: 0em 0 2em;
	}
	h2 {
		margin: 0.5em 0;
	}
</style>
